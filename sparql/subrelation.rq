prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>
select ?subclass_of_thing (count(?subsubclass) as ?count_subsubclass) where {
  ?subclass_of_thing rdfs:subClassOf owl:Thing .
  ?subsubclass rdfs:subClassOf ?subclass_of_thing .
} group by ?subclass_of_thing order by desc(?count_subsubclass)
